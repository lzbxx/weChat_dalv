page{
    background: #fff
}

.main{
    height: 100%;
    width: 100%;
    box-sizing: border-box;
    padding: 0 75rpx;
    text-align: center;
    
}
.main button{
    margin-top: 60rpx;
    border: width border-style color;
     border-radius: 8%/50%;
      background: #fff
}
.main button:nth-of-type(1){
    width: 100%;
    height: 96rpx;
    border: 1rpx solid #4D65F3;
    color: #4D65F3
   
}
.main button:nth-of-type(2){
    width: 100%;
    height: 96rpx;
    background:  #4D65F3;
    color: #fff
   
}
.main text{
    display: block;
    margin-top: 100rpx;
    color: #606060;
    font-size: 28rpx
}